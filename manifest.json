{
  "manifest_version": 2,
  "name": "SongId4",
  "version": "4.0",
  "description": "Music recognition. Like shazam or soundhound.",
  "homepage_url": "https://github.com/Nerixyz/SongId4",
  "icons": {
    "16": "icons/icon.svg",
    "48": "icons/icon.svg",
    "96": "icons/icon.svg"
  },
  "background": {
	  "scripts": ["/background/inject.js"]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "singid4@nerixyz"
    }
  },
  "browser_action": {
	  "browser_style": true,
	  "default_title": "Identify",
	  "default_icon": "icons/icon.svg"
  },
  "permissions": [
    "activeTab",
    "*://*.acrcloud.com/v1/identify",
    "storage"
  ],
  "options_ui": {
    "page": "options/options.html"
  }
}
